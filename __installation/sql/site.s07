CREATE TABLE `#__kunena_messages_text` (   `mesid` int(11) NOT NULL DEFAULT '0',   `message` text NOT NULL,   PRIMARY KEY (`mesid`) ) ENGINE=InnoDB DEFAULT CHARSET=utf8;
INSERT INTO `#__kunena_messages_text`  VALUES ('1', '[size=4][b]Üdvözöli a Kunena![/b][/size]⏎\n⏎\nKöszönjük, hogy a Kunena-t választottad a Joomla közösséged fórumaként.⏎\n⏎\nA Kunena a Swahili fordítása, és azt jelenti, “beszélni”. professzionális nyílt-forráskódú fejlesztők készítették azzal a céllal, hogy a legjobb minőségű megoldást nyújtsák a Joomla-nak.⏎\n⏎\n⏎\n[size=4][b]Egyéb Kunena források[/b][/size]⏎\n⏎\n[b]Kunena dokumentáció:[/b] [url]http://www.kunena.org/docs[/url] ⏎\n⏎\n [b]Kunena támogatói fórum[/b]: [url]http://www.kunena.org/forum[/url] ⏎\n⏎\n [b]Kunena letöltések:[/b] [url]http://www.kunena.org/download[/url] ⏎\n⏎\n [b]Kunena blog:[/b] [url]http://www.kunena.org/blog[/url] ⏎\n⏎\n [b]Kunena a Twitteren:[/b] [url]http://www.kunena.org/twitter[/url]'), ('3', 'Ide várom a program jobbítását célzó ötleteket, javaslatokat'), ('4', 'Tényleg nem olvasható a .pdf. Szándékos? üdv Gábor'), ('5', 'Kösz a figyelmeztetést.\r\nEz a pdf tartalmilag nem tartozik ide, éppen csak probálgattam ezt a funkciót, mindjárt megpróbálom a nyomait is eltakarítani....'), ('9', 'Úgy látszik tökéletes a program, semmi javítani való, csiszolni való nincs rajta..mivel senki nem ír ide semmit. Vagy mindenki nyaral?'), ('10', 'Nyár van :)'), ('11', 'Akkor mindenki dobja be, aminek szerinte itt szerepelni kell.'), ('12', 'Még csak próba :)'), ('13', 'Elsőként talán azt: ki szeretne a programíró munkacsoportba kerülni?\r\nÉs hogy működne a csoport illetve a programírás?'), ('14', 'Szerintem először ide kell hozni ide embereket. Ha működik, akkor érdemes beszélni róla. Szerintem  a programírás az ötletbörzével indul.'), ('15', 'Ja..persze azok közül, akik olvassák :)'), ('16', 'Hát, ide kell vonni az embereket. \r\nMinek kell benne lenni a programban?\r\n\r\nBevezető, munkásmozgalmi demokratikus örökség.\r\nKonkrét követelések, kollektív tulajdon arányának nevelése, köz és magán szétválasztása , ezzel a.korrupció háttérbe szorítása,'), ('17', '[ul]\r\n  [li]likvid demokrácia[/li]\r\n  [li]feltétel nélküli alapjövedelem[/li]\r\n  [li]államadóság visszafizetésének megtagadása[/li]\r\n  [li]kilépés a NATO -ból, semlegesség, pacifizmus[/li]\r\n  [li]közösségi termelési módok bevezetése (szövetkezetek)[/li]\r\n  [li]munkahelyi demokrácia[/li]\r\n  [li]egymilliárd feletti  magántulajdonban lévő termelő vagyonok kártalanitás nélküli kisajátitása és szövetkezetté alakitása[/li]\r\n  [li]pénzreform, közösségi bank rendszer, alternativ pénz[/li]\r\n[/ul]'), ('18', 'Hosszú, közép és rövid távú program.\r\nPolitikai, stratégiai és taktikai szint.\r\n\r\nKérdés: melyiket akarjuk most megírni?\r\nA másik kettőre utalni kell, mindenesetre.'), ('19', 'Ahhoz, hogy programunk lehessen, először céljaink kell legyenek!\r\n\r\nHosszú, közép és rövid távú célok (célrendszer).\r\nEzek elérését kell szolgálja a pártprogram!\r\nVégső soron határidőkkel.'), ('20', 'András! Ha neked ez a szohasználat megfelelöbb akor legyen, amit irtam azok középtávu célok. Hosszu távu cél a kommunizmus.'), ('21', 'Amiket írtál, Tibor, azok nem célok, hanem célok megvalósítására alkalmas (annak szánt) eszközök.\r\n\r\nRövid távú cél lehet pl. a jelenleg a fizikai-biológiai minimum határán élő kb 1 millió ember megmentése a haláltól. (szoc önvédelem) Középtávú cél lehet életszínvonaluk emelése a létminimumig illetve afölé.\r\n\r\nÉs így tovább..'), ('22', 'András!  További nyelvészkedésben, szó értelmezésben nem kivánok részt venni. Várom a tartalmi, lényegi véleményeket.'), ('23', 'Az nem nyelvészkedés, ha a célokat és az eszközöket nem keverjük össze, hanem határozottan elkülönítjük! Ennek egy programban látszania kell!\r\n\r\nEgyikből következik a másik: ok és okozat.. ok nélküli okozat a levegőben lógó dolog.'), ('30', 'A részleteket itt vitassuk meg!\r\n\r\nMindenkinek ajánlott, a munkacsoport tagjainak [b]kötelező![/b]'), ('31', 'A részletek megvitatására.\r\n\r\nMindenkinek ajánlott, de a munkacsoport tagjainak [b]kötelező![/b]'), ('32', 'Mint már többször irtam heti egy délelötti ügyeletet vállalok a NET pontban (09:00 - 14:00)\r\nKI fogja ennek az ügynek a koordinálását végezni? Várom az értesitést, hogy melyik nap lesz ez.\r\n\r\nMegjegyzés? Biztos, hogy ennek a témának itt, a BAL-Párt pártprogram alatt van a helye?'), ('35', 'Jó lenne egyszer végigvitatni! \r\nMinden alkalommal csak nekirugaszkodunk és annyiban marad.\r\n\r\nAz első javaslatom: tisztázni, milyen [b]célok[/b] elérésére kívánjuk az FNA-t mint [b]eszközt [/b]bevetni, bevezetni?\r\n\r\nSzerintem ez a cél középtávon (stratégiailag): az emberhez méltó élet minimumának (szélesen értelmezett létminimumnak) biztosítása a társadalom minden tagja részére.\r\nAzoknak is, akiknek a rendszer (kapitalizmus) nem kínál fel munkalehetőséget, ezzel jövedelmet, azaz megélhetést.\r\n(Más megközelítésben. a leépülést, a lassú vagy gyors pusztulást kínálja fel.)\r\n\r\nA stratégiai célból eredő rövid távú (taktikai) cél: a ma és itt létükben fenyegetett honfitársaink megmentése a (lassabb vagy gyorsabb) pusztulástól.\r\nKiinduló adatnak fogadjuk el, hogy ma Magyarországon kb 4,5 millió ember él a létminimum alatt. Közülük gyors pusztulásra van ítélve legalább 1 millió ember ( a fizikai-biológiai minimumopn élnek), lassú pusztulásra további lb 2 millió. Ebbe a 3 millióba bármikor lecsúszhat a létminimum alatt élő, kevésbé rossz helyzetű 1,5 millióból bárki, bármely család.\r\n\r\nKérdés, hogy ez a rövid távú cél elérhető-e az FNA bevezetésével?\r\nsemmiképpen, hiszen az - ha átvihető lenne is, ami még sehol nem sikerült, a legfogékonyabb centrumországokban sem - időben hosszú, évtizedes program. (Jó esetben.)\r\nEzeknek az embereknek már nincs 10 évük!!!\r\n\r\nHa a rövid távú célt az FNA követelése nem teljesítheti, akkor [b]az FNA követelése önmagában optimális esetben sem elégséges.[/b]\r\nÉs akkor még nem beszéltünk arról, hogy\r\n\r\n- a középtávú cél elérésére ez-e az egyetlen lehetséges eszköz?\r\n- melyek bevezetésének feltételei?\r\n- [b]egyenlő-e az FNA azzal az egyetlen technikai megoldásjavaslattal[/b], amelyet FNA-ként emlegetnek hazai hívei? \r\n- csak azok- e a hatásai, amelyeket az FNA-val kapcsoltban emlegetni szokás?'), ('36', 'A részleteket itt vitassuk meg!\r\n\r\nMindenkinek ajánlott, a munkacsoport tagjainak kötelező!'), ('37', 'Mint már többször irtam heti egy alkalomra délelötti ügyeletet vállalok (09:00-14:00). Várom a visszajelzést melyik nap lesz ez, hogyan kapom meg a kulcsot?'), ('38', 'András!  Előszór is ajánlom figyelmedbe: \r\n\r\n[url=Szombati esemény]https://www.facebook.com/events/644565728996521/?ref_dashboard_filter=upcoming[/url]\r\n\r\nKérdéseid természetesen jogosak. \r\n\r\nPéldául én is tartok tőle, hogy egy szerényebb összegű FNA bevezetése nagyszerű hivatkozást ad a mindenkori hatalomnak, hogy minden szociális juttatást megvonjon, az egészségügyet fizetőssé tegye stb, stb.\r\n\r\nEZÉRT IS UTASITANDÓ EL A KIS ÖSSZEGÜ \"LÉT\" KONCEPCIÓ. \r\n\r\nTermészetesen az FNA nem szünteti meg a tőke-munkás alapvető érdekellentétet és a tőkés állam - az emberek nagy többsége közti ellentétet, az osztályharcot nem teszi \r\nfeleslegessé.\r\n\r\nAzt sem állítom, hogy ez az egyetlen megoldás. Viszont más a gyakorlati megvalósíthatóság halvány esélyével is kecsegtető javaslattal senki nem állt eddig elő! (Az hogy meg kell haladni a kapitalizmust nem javaslat hanem vágy).\r\n\r\nAz FNA pedig egy - ha nagy nehézségek árán is - de gyakorlati megvalósíthatóság reményével is kecsegtető konkrét javaslat. Szerintem éppen az benne a nagyszerű, hogy látszólag a piacgazdaság, kapitalizmus keretein belül marad, ezért nem teljesen kizárt, hogy a jelen hatalom is elfogadja ha nagy társadalmi nyomást érez rá. Ugyanakkor meggyőződésem szerint bevezetése esetén hosszabb távon belülről felőröli a kapitalizmus rendszerét (mint ahogy a sziklán folyó víz látszólag kitér a szikla ellenállása elől, de valójában végül bele vájja a grand kanyont). De ez t az utóbbi gondolatomat taktikai okokból most nem kell hangoztatni.\r\n\r\nNagy gond az FNA -val persze az is, hogy törvényekkel kell bevezetni és mint tudjuk a törvényeket a hatalom megváltoztathatja. Tehát a folyamatos társadalmi ellenőrzés, társadalmi nyomás fenntartása az FNA védelmében folyamatos feladat lesz a társadalom számára. De ez nem úszható meg semmiképpen amíg kapitalizmus van - sőt utána sem; mindaddig amíg bármiféle hatalom létezik.\r\n\r\nSzámomra nagyon szimpatikus az FNA azon megközelítése is, hogy a tényleges társadalmi - termelési viszonyok megváltoztatását tűzi ki célul és nem a közvetlen  politikai hatalom megszerzését. Marx óta tudjuk, hogy a politika a FELÉPITMÉNY része, az ALAPBA többek között a termelési viszonyok tartoznak. Természetesen dialektikusan szemlélve az FNA bevezetése nem megy a politikai szándék. az ezt kikényszerítő  alulról történő sikeres nyomás nélkül, de fordítva a politikai hatalom megszerzése sem segít a dolgokon  az ALAPOK megváltoztatása nélkül. \r\n\r\nHa időd engedi olvasd el Kapitány Gábor és Kapitány Ágnes \"Szellemi termelési mód\" című könyvét (nem könnyű olvasmány időt kell rá szánni - EPUB -ban el tudom küldeni). Erről a könyvről váltottam pár szót Gödön Kis Viktorral Ő is nagyon figyelemre méltónak találta. Nekem nagyon tetszik ez a könyv. Az FNA koncepció ezzel a szellemi termelési mód koncepcióval összekapcsolva mintha egy lehetséges távlati kibontakozás lehetőségét vetné fel......\r\n\r\nA legszegényebb tömegek természetesen nem várhatnak 10 évet. Egyrészt nem látom be miért kellene az FNA bevezetéséhez 10 év...nem annyira bonyolult dolog ez. Másrészt természetesen  fenn kell tartani az egyéb szociális ellátó rendszereket is, átmeneti időszakban feltétlenül, de kisebb kapacitással még az után is mert vannak olyan fokozottan hátrányos helyzetű emberek (pl. súlyos testi fogyatékosságok) akiknek az életben maradása messze az átlagjövedelemnél (tehát az FNA összegénél is)  magasabb ráfordításokat igényel. Természetesen egy humánus társadalom ezeket a tagjait sem hagyhatja meghalni.\r\n\r\nNAGYON FONTOS ALAPELV: AZ FNA SEMMIKÉPPEN SEM LEHET INDOK AZ EGÉSZSÉGÜGYI ELLÁTÁS FIZETŐSSÉ TÉTELÉRE!\r\n\r\nAz FNA filnmen vázolt egyes technikai részletekkel kapcsolatban (például az adórendszerre tett javaslataival kapcsolatban) nekem is erős ellenérzéseim vannak. Szerintem sem kell a film javaslatainak minden részletéhez ragaszkodni, \r\n\r\nPéldául én azt sem értem, hogy az FNA hívei miért zárkóznak el kategorikusan az FNA egy részének természetbeli juttatás formájában való megvalósításától? (pl. városiaknak ingyenes közösségi közlekedés, vidékieknek ingyenes vasút, volán használat, ingyenes posta szolgáltatások, ingyenes NET stb)\r\n\r\nÖsszefoglalva maga az FNA alapelv nagyon is baloldali, előre mutató, támogatható.\r\n\r\nUtopszkij'), ('39', 'A munka során készült különböző közbenső komplett munka változatok\r\n\r\n(A legfrisseb a lista végén van)'), ('40', 'MUNKÁT!  OTTHONT! I TTHON!    1.00   2014.09.17\r\n\r\nMagyarország Demokratikus és Szociális Újjáépítésének programja\r\n\r\n(tervezet)\r\n\r\n \r\n\r\nA Balpárt modern, baloldali politikai szervezet, amely őrzi a hagyományos munkásmozgalmi értékeket, de tevékenysége során mindvégig a legkorszerűbb elvek alapján végzi munkáját.\r\n\r\n \r\n\r\nA nemzetköziség értékét valló párt vagyunk. Testvérpártjaink minden országában jelen vannak és velünk együtt küzdenek egy szociálisan elkötelezett, igazságos és élhető világért.\r\n\r\n \r\n\r\nDemokratikus és szociális ellenállási mozgalom vagyunk. A harmadik magyar köztársaság megbukott, a demokrácia zsákutcába került, a demagóg jobboldalnak valódi ellenzéke nincs és egyre nagyobb a magyar fasiszta pártok támogatottsága.\r\n\r\nA Balpárt legfőbb célja létbiztonságot adni, egy olyan gazdasági és társadalmi rendszert kialakítani, ami nem terheli a környezetet és mindenki számára biztosítja a megélhetéshez, a lakáshoz, élelmiszerhez, iskolához és közlekedéshez való hozzáférést.\r\n\r\n \r\n\r\nVan kiút a demokrácia és a baloldal válságából!\r\n\r\nA magyar baloldalinak mondott pártok nem baloldaliak. Polgári demokrata formációk bitorolják a baloldalt immár negyedszázada. Ezek a pártok háromszor csapták be választóikat, akik joggal fordultak el a neoliberális politikát képviselő politikusoktól. Megszűnt minden harmadik munkahely, a közvagyont elherdálták, indokolatlan beruházásokkal eladósították az országot, megszüntették az ingyenes oktatási és egészségügyi ellátórendszert és máig ható társadalmi válságot idéztek elő a devizahitelezés ösztönzésével. A hazánkban élő emberek jelentős része tart attól, hogy a baloldalinak hazudott érdekcsoportok újra a hatalom közelébe kerülve antiszociális ámokfutásba kezdhetnek. Tömegeket az ettől való félelem vitt a demagóg Fidesz és a Jobbik támogatóinak táborába.\r\n\r\nLátjuk azonban, hogy Magyarországon továbbra is jelentős igény van a szociális igazságosságra, a mindennapok biztonságára, a munka világának kiszámíthatóságára.\r\n\r\nA demokrácia válságából kiutat csak egy új, hiteles elvi politikát képviselő baloldali párt tud mutatni. Egy olyan párt, amely a dolgozó vagy dolgozni vágyó, de munkát nem találó többséget képviseli! Egy olyan párt, amely nemet mond az országot reménytelen helyzetbe taszító politikusi és gazdasági körök szabadrablására! Egy olyan párt, amely nem a szegények szegények elleni háborúját ösztöni! Egy olyan párt, amely nemet mond a szélsőséges kizsákmányolásra, amelynél az ember az első, nem a profit!\r\n\r\nA Balpárt megfelel ezeknek az elvárásoknak. Mi megalkuvás nélkül küzdünk a kormányon lévő jobboldallal és a látszólag annak ellenzékét jelentő szélsőjobboldallal. Bátran kimondjuk, hogy a 2010 előtti időszak visszahozatala nem lehet célunk, így azokból az emberekből sem kérünk, akik szélsőséges ámokfutásukkal, a mindent átszövő korrupcióval lejáratták a demokráciát és a baloldalt.\r\n\r\n\r\nSzociális Köztársaságot!\r\n\r\nA rendszerváltás kapitalistának sem nevezhető torz, feudális, a közvagyon magánosítására épülő modellje megbukott. Miközben azt hazudták, hogy mindenki nyertes lesz, valójában egy maréknyi kisebbségen kívül szinte mindenki a vesztesek közé került.\r\n\r\nMa már egyértelmű, hogy új társadalmi és gazdasági, Szociális Köztársaságra berendezkedésre van szükség.\r\n\r\n\r\nForradalmat a fejekben, bátor tettekre van szükség!\r\n\r\nA magyar társadalom mély apátiában vergődik évtizedek óta, az emberek elvesztették a hitüket, szinte teljesen eltűntek valódi és radikális társadalmi változásokat követelő mozgalmak. A kormány manipulálja a társadalmat, a szélsőjobb a gyűlöletre épít, a demokratikusnak mondott ellenzék pedig csak a 2010. előtti, már egyszer megbukott rendszer restaurációjában képes gondolkodni.\r\n\r\nÚj, közösségi elvek alapján alulról építkező, önszerveződő érdekvédelmi mozgalmakra van szükség. A Szociális Köztársaság egy olyan decentralizált rendszer, ahol a társadalom tagjai nem pusztán négyévente, hanem folyamatosan élnek demokratikus jogaikkal, nem mondnak le az érdekeik képviseletéről, hanem közvetlenül részt vesznek a döntések előkészítésében is. Egy olyan rendszer, ahol a többség politikai és gazdasági öntudatát nem lehet befolyásolni.\r\n\r\nA Balpárt nem élharcosa, hanem pusztán segítője azon forradalmi változásoknak, amelyek ahhoz szükségesek, hogy valóban radikális változások történjenek Magyarországon.\r\n\r\n \r\n\r\n \r\n\r\nMUNKÁT!\r\n\r\n \r\n\r\nA Balpárt hagyományos baloldali értékek mentén politizál, így kiemelt szerepet szánunk a munkának annak társadalmi szervező ereje miatt. A munka fogalma alatt azonban nem érthetjük pusztán a kapitalizmus által megteremtett, a kizsákmányolásra és profittermelésre koncentráló munkát. Munkának tekintünk minden olyan tevékenységet, amelynek célja az, hogy a közösség tagjai megéljenek, művelődjenek vagy szórakozzanak. Ennek megfelelően számtalan olyan társadalmilag hasznos tevékenység van, amelyek a jelenlegi torz logika szerint nem termelnek jövedelmet annak ellenére sem, hogy igenis jelentősen hozzájárulnak ahhoz, hogy egészséges társadalmi és természeti környezetben éljünk.\r\n\r\n \r\n\r\nElső lépés, az elszámoltatás\r\n\r\nSenki sem gondolhatja azt, hogy radikális társadalmi változtatások végrehajthatóak elszámoltatás nélkül. Magyarország kifosztása évtizedek óta szisztematikusan zajlik, abban a gazdasági és politikai elit szinte egésze érintett. A hatalmas magánvagyonok, a külföldre menekített ezermilliárdokról pontos képet kell kapjunk és el kell érni, hogy ezek jelentős része visszatérjen, újra közvagyon legyen. A kisajátítók kisajátítását igenis napirendre kell venni, senki se gondolja, hogy az új Szociális Köztársaságban elfogadjuk a múlt igazságtalanságait és minden retorzió nélkül menedzseljük majd a ránk hagyományozott csődtömeget. Nem nyúlunk hátra, de igenis tudnia kell mindenkinek, hogy innentől kezdve a közvagyonból való lopás a legsúlyosabb bűncselekménynek számít! \r\n\r\n\r\nTámogatjuk a Feltétel Nélküli Alapjövedelem bevezetését\r\n\r\nA szolidáris társadalmi rendben senki sem maradhat élelem, meleg otthon, az internet nyilvánossága nélkül. Az ehhez szükséges javakat a társadalom tagjai maguknak megtermelik, úgy, hogy közben az adózási rendszeren keresztül gondoskodnak arról is, hogy a közösségi szolgáltatások is elérhetőek legyenek. A hagyományos munkásmozgalom legfőbb törekvése mindenkoron az volt, hogy munkanélküliség, betegség vagy öreg kor esetén is a társadalom tagjai rendelkezzenek a tisztes létfenntartáshoz szükséges jövedelemmel. Munkanélküli ellátással, táppénzzel vagy nyugdíjjal. A Feltétel Nélküli Alapjövedelem egy koncepció, amelynek lényege, hogy kiváltja a sok néven nevezett ellátásokat egyetlen fix jövedelemmel, így nem lenne szükség családi pótlékra, társadalombiztosításra, nyugdíjra, a számtalan néven futó segélyekre. A koncepció jó és perspektivikus, de amíg pontosan nincs kidolgozva ez a dolog, amíg a bevezetése csak torzított formában merül fel, addig nem támogatjuk, hogy „FNA-jellegű” kezdeményezésekre hivatkozva bármilyen jóléti ellátást megvonjanak. Amíg az FNA nem realizálódik, addig támogatjuk egy sávosan biztosított munkanélküli segélyt, amely egy évig a korábbi jövedelem száz százaléka, másfél évig nyolcvan százaléka, és három évig évig pedig ötven százaléka kell legyen. Fél év munkanélküliség után a közösségi munkaügyi szolgáltatónak komplex képzést kell biztosítani, ezt követően pedig legalább egy évig tartó közérdekű foglalkoztatást.\r\n\r\n \r\n\r\nSzociális Gazdaság alapjait kell lerakni\r\n\r\nAz ország nagyobb része súlyos szociális és gazdasági válság sújtotta terület. Ezeken a területeken semmi értelme a kapitalista logika szerinti beruházásösztönző tevékenységeknek, mert a tőke nem jut el ide. Ezeken a területeken szövetkezeti alapon szerveződő szociális gazdaság beindítására van szükség, azaz a munkahelyteremtés központi szerepet kap, de csak úgy, hogy előtte elindultak helyi összefogások, amelyeknek célja a tudatos és célirányos helyi gazdasági és kulturális fejlesztés. A feladat az ehhez szükséges anyagi alapok megteremtése, a beruházások ösztönzése források és például földalapok megteremtésével. Célunk, hogy a gazdasági működés során a termelőeszközök mind nagyobb hányada kerüljön közösségi, elsősorban szövetkezeti tulajdonba.\r\n\r\n \r\n\r\nÁllamadósság\r\n\r\nHazánk torz és méltatlan helyzetbe kerülésének egyik oka az indokolatlan és kamatos kamat következtében meglévő államadósság. A fejlődés egyik gátjának tekintjük, hogy hazánk bevételeinek jelentős része a nem valóságos gazdaságba vagy szociális ellátásra folyik, hanem nemlétező és értéket nem termelő pénzintézetek, bankok, világszervezetek finanszírozásáa folyik el. A Balpárt egyik első intézkedéseként újratárgyalná az államadósság mértékét és visszafizetési ütemét. Adósságunk csökkentése lételemünk egyik bázisa kell legyen.\r\n\r\n \r\n\r\nA közösségi közlekedés reformja, segíteni kell a munkába jutást\r\n\r\nA magyar gazdaság válságának egyik legfőbb oka a szabadrablás mellett az volt, hogy az illetékesek piaci alapokra próbálták helyezni a közlekedési vállalatokat, a kieső adóbevételeket pedig a benzinárhoz csapott ilyen-olyan címkékkel ellátott adófajtákkal próbálták pótolni.\r\n\r\nA környezetvédelem ügye mellett elkötelezettek vagyunk, így nem támogatjuk a radikális üzemanyagár csökkentést, azonban a közösségi közlekedés átfogó reformjával segíteni kívánjuk a munkavállalók mobilitásának növekedését. Kilencven százalékos közösségi támogatásra van szükség minden utazás alkalmával. Középtávon pedig biztosak vagyunk benne, hogy a teljesen ingyenes tömegközlekedésé a jövő.\r\n\r\n \r\n\r\nErős szakszervezetekre van szükség\r\n\r\nTíz főnél több embert foglalkoztató munkahelyeken, vállalkozásoknál kötelezően meg kell alakítani a szakszervezeteket. A kor igényeihez illeszkedő módon az ágazati szakszervezetek mellett szükséges a területi szinten is erősíteni a szakszervezeteket. A munkavállalók kötelező szakszervezeti tagságának díját a munkáltatóknak kell befizetnie az országos szakszervezeti alapba, amelyet országos, titkos választás útján választanak meg és egyben felügyeli a társadalombiztosítási és a nyugdíjbiztosítási rendszert is. A Szakszervezeti Tanács választott tisztségviselőkből álló testületei vannak regionális és térségi szinteken is.\r\n\r\n \r\n\r\n \r\n\r\nOTTHONT!\r\n\r\nA Balpárt szerint a fűtött, egészséges otthon alapjog. Az elmúlt negyedszázadban százezrek vesztették el lakásukat, házukat, milliós azok tábora, akik képtelenek megfizetni a lakhatás költségeit és egyre nyilvánvalóbban fenyegeti őket otthonuk elvesztése. Szintén százezres azok tábora, akiket felkészületlenül ért a bankok szédítő reklámhadjárata és felelőtlenül vettek fel devizakölcsönt.\r\n\r\n \r\n\r\nA fiatalok többségének esélye sincs arra, hogy olyan jövedelmet szerezzen, amelyből otthont tartson fenn, ezért többségük külföldre vándorol vagy kényszerből szüleivel él.\r\n\r\n \r\n\r\nEzen reménytelen helyzetekből kiutat kell találni. Nem pusztán anyagi helyzettől függ, hogy kinek lehet és kinek nem lehet otthona. Az önkormányzatok többsége rossz gazdájának bizonyult a közösségi bérlakásoknak. Azok jelentős részét koncepció nélkül értékesítette, sokszor felmerült a korrupciógyanú. Az is gyakori probléma ezen a területen, hogy a rászorulókat szándékosan szorítják ki az önkormányzati bérlakásokból. A Szakszervezeti Tanács által felügyelt és irányított Országos Szociális Bérlakás Hivatal felállítására van szükség, mely koordinálja a meglévő lakások igazságos felhasználását és új lakások felépítését kezdeményezi.\r\n\r\n \r\n\r\n \r\n\r\n \r\n\r\nITTHON!\r\n\r\n \r\n\r\nA magyar történelem oly gyakran ismétlődő válsághelyzeteiben a reménytelen helyzetbe taszított tömegek százezres nagyságrendben menekültek külföldre. Ma sincs ez másként. Mintegy félmillió ember honfitársunk keresi a boldogulás lehetőségét külföldön. Többségükben fiatalok, akik itthon képtelenek voltak fenntartani magukat és családjukat, képtelenek voltak otthont vásárolni vagy bérelni. Azért menekültek el, mert Magyarországon ellopták a jövőjüket. Őket egy torz és tolvaj politikai és gazdasági elit egyeduralma hajtotta a határainkon túlra. Ha ők tartósan kint maradnak, akkor az rendkívül komoly társadalmi katasztrófát fog okozni Magyarországon, mert fel fog borulni társadalmi újraelosztási rendszer egésze. Mindent meg kell tenni azért, hogy a szociális migráció megálljon, a fiatal generációk itthon tudjanak boldogulni és gyarapodni. Ehhez szükséges, hogy a nyugati és keleti bérek értéke közelítsen egymáshoz. A Balpárt nemzetközi testvárpártjaival együtt többek között ezen a megoldáson is dolgozik európai síkon.'), ('41', 'MUNKÁT! OTTHONT!  ITHON!   1.01   2014.09.19\r\n\r\n\r\n\r\nBalpárt:\r\n\r\n\r\n\r\n\r\n\r\nMUNKÁT! \r\nOTTHONT! \r\nITTHON!\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\nMagyarország Demokratikus és Szociális Újjáépítésének programja\r\n(tervezet)\r\nA Balpárt modern, baloldali politikai szervezet, amely őrzi a hagyományos munkásmozgalmi értékeket, de tevékenysége során mindvégig a legkorszerűbb elvek alapján végzi munkáját.\r\nA nemzetköziség értékét valló párt vagyunk. Testvérpártjaink minden országában jelen vannak és velünk együtt küzdenek egy szociálisan elkötelezett, igazságos és élhető világért.\r\nDemokratikus és szociális ellenállási mozgalom vagyunk. A harmadik magyar köztársaság megbukott, a demokrácia zsákutcába került, a demagóg jobboldalnak valódi ellenzéke nincs és egyre nagyobb a magyar fasiszta pártok támogatottsága. \r\nA Balpárt legfőbb célja létbiztonságot adni, egy olyan gazdasági és társadalmi rendszert kialakítani, ami nem terheli a környezetet és mindenki számára biztosítja a megélhetéshez, a lakáshoz, élelmiszerhez, iskolához és közlekedéshez való hozzáférést.\r\n\r\n\r\nVAN KIÚT A DEMOKRÁCIA ÉS A BALOLDAL VÁLSÁGÁBÓL!\r\n\r\nA magyar baloldalinak mondott pártok nem baloldaliak. Polgári demokrata formációk bitorolják a baloldalt immár negyedszázada. Ezek a pártok háromszor csapták be választóikat, akik joggal fordultak el a neoliberális politikát képviselő politikusoktól. Megszűnt minden harmadik munkahely, a közvagyont elherdálták, legtöbbször indokolatlan beruházásokkal eladósították az országot, lényegében megszüntették az ingyenes oktatási és egészségügyi ellátórendszert és máig ható társadalmi válságot idéztek elő a devizahitelezés ösztönzésével. A hazánkban élő emberek jelentős része tart attól, hogy a baloldalinak hazudott érdekcsoportok újra a hatalom közelébe kerülve antiszociális ámokfutásba kezdhetnek. Tömegeket az ettől való félelem vitt a demagóg Fidesz és a Jobbik támogatóinak táborába. Meg kell mutatni, hogy van hiteles, támogatásra méltó baloldal is az országban.\r\nEgyértelműen látjuk, hogy Magyarországon továbbra is jelentős igény van a szociális igazságosságra, a mindennapok biztonságára, a munka világának kiszámíthatóságára.\r\nA demokrácia válságából kiutat csak egy új, hiteles elvi politikát képviselő baloldali párt tud mutatni. Egy olyan párt, amely a dolgozó vagy dolgozni vágyó, de munkát nem találó többséget képviseli! Egy olyan párt, amely nemet mond az országot reménytelen helyzetbe taszító politikusi és gazdasági körök szabadrablására! Egy olyan párt, amely nem a szegények szegények elleni háborúját ösztönzi, hanem a nemzetek felett álló szolidaritás jelszavát tűzi zászlajára Egy olyan párt, amely nemet mond a szélsőséges kizsákmányolásra, amelynél az ember az első, nem a profit!\r\nA Balpárt megfelel ezeknek az elvárásoknak. Mi megalkuvás nélküli ellenfelei vagyunk a kormányon lévő jobboldalnak és az annak csak látszólag ellenzékeként tevékenykedő szélsőjobboldalnak. Kimondjuk azt is, hogy a 2010 előtti időszakhoz visszatérni nem lehet, mi természetesen azokból az emberekből sem kérünk, akik szélsőséges ámokfutásukkal, a mindent átszövő korrupcióval lejáratták a demokráciát és a baloldalt.\r\nMUNKÁT!\r\n\r\nA Balpárt hagyományos baloldali értékek mentén politizál, így kiemelt szerepet szánunk a munkának annak társadalmi szervező ereje miatt. A munka fogalma alatt azonban nem érthetjük pusztán a kapitalizmus által megteremtett, a kizsákmányolásra és profittermelésre koncentráló munkát. Munkának tekintünk minden olyan tevékenységet, amelynek célja az, hogy a közösség tagjai megéljenek, művelődjenek vagy szórakozzanak. Ennek megfelelően számtalan olyan társadalmilag hasznos tevékenység van, amelyek a jelenlegi torz logika szerint nem termelnek jövedelmet annak ellenére sem, hogy igenis jelentősen hozzájárulnak ahhoz, hogy egészséges társadalmi és természeti környezetben éljünk.\r\n\r\n1. Első lépés: Elszámoltatás!\r\nSenki sem gondolhatja azt, hogy radikális társadalmi változtatások végrehajthatóak elszámoltatás nélkül. Magyarország kifosztása évtizedek óta szisztematikusan zajlik, abban a gazdasági és politikai elit szinte egésze érintett. A hatalmas magánvagyonokról, a külföldre menekített ezermilliárdokról pontos képet kell kapjunk és el kell érni, hogy ezek jelentős része visszatérjen, újra közvagyonná legyen. A kisajátítók kisajátítását igenis napirendre kell venni, senki se gondolja, hogy az új Szociális Köztársaságban elfogadjuk a múlt igazságtalanságait és minden retorzió nélkül menedzseljük majd a ránk hagyományozott csődtömeget. Nem nyúlunk hátra, de igenis tudnia kell mindenkinek, hogy innentől kezdve a közvagyonból való lopás a legsúlyosabb bűncselekménynek számít! \r\n\r\n2. Szociális Köztársaságot!\r\nA rendszerváltás kapitalistának sem nevezhető torz, feudális, a közvagyon magánosítására épülő modellje megbukott. Miközben azt hazudták, hogy mindenki nyertes lesz, valójában egy maréknyi kisebbségen kívül szinte mindenki a vesztesek közé került.\r\nMa már egyértelmű, hogy új társadalmi és gazdasági, Szociális Köztársaságra berendezkedésre van szükség.\r\n\r\n3. Forradalmat a fejekben, bátor tettekre van szükség!\r\nA magyar társadalom mély apátiában vergődik évtizedek óta, az emberek elvesztették a hitüket, szinte teljesen eltűntek valódi és radikális társadalmi változásokat követelő mozgalmak. A kormány manipulálja a társadalmat, a szélsőjobb a gyűlöletre épít, a demokratikusnak mondott ellenzék pedig csak a 2010. előtti, már egyszer megbukott rendszer restaurációjában képes gondolkodni.\r\nÚj, közösségi elvek alapján alulról építkező, önszerveződő érdekvédelmi mozgalmakra van szükség. A Szociális Köztársaság egy olyan decentralizált rendszer, ahol a társadalom tagjai nem pusztán négyévente, hanem folyamatosan élnek demokratikus jogaikkal, nem mondnak le az érdekeik képviseletéről, hanem közvetlenül részt vesznek a döntések előkészítésében is. Egy olyan rendszer, ahol a többség politikai és gazdasági öntudatát nem lehet befolyásolni.\r\nA Balpárt nem élharcosa, hanem pusztán egyik segítője azon forradalmi változásoknak, amelyek ahhoz szükségesek, hogy valóban radikális változások történjenek Magyarországon.\r\n\r\n4. Szociális Gazdaság alapjait kell lerakni\r\nAz ország nagyobb része súlyos szociális és gazdasági válság sújtotta terület. Ezeken a területeken semmi értelme a kapitalista logika szerinti beruházásösztönző tevékenységeknek, mert a tőke nem jut el ide, még akkor sem, ha sokan ebben reménykednek. Ezeken a területeken szövetkezeti alapon szerveződő szociális gazdaság beindítására van szükség, azaz a munkahelyteremtés központi szerepet kell kapjon, de csak úgy, hogy előtte elindultak helyi összefogások, amelyeknek célja a tudatos és célirányos helyi gazdasági és kulturális fejlesztés. Nem az elmaradott területen ügyeskedő gazdagokat, hanem a valóban rászoruló közösségeket kell támogatni. \r\nA feladat az ehhez szükséges anyagi alapok megteremtése, a beruházások ösztönzése források például földalapok megteremtésével. \r\nCélunk, hogy a gazdasági működés során a termelőeszközök mind nagyobb hányada kerüljön közösségi, elsősorban szövetkezeti tulajdonba.\r\n\r\n5. Erős szakszervezetekre van szükség, Országos Szakszervezeti Alapot kell létrehozni\r\nTíz főnél több embert foglalkoztató munkahelyeken, vállalkozásoknál kötelezően meg kell alakítani a szakszervezeteket, minden dolgozó automatikusan szakszervezeti tag kell legyen. A kor igényeihez illeszkedő módon az ágazati szakszervezetek mellett szükséges a területi szinten is erősíteni a szakszervezeteket. \r\nA munkavállalók kötelező szakszervezeti tagságának díját a munkáltatóknak kell befizetnie az Országos Szakszervezeti Alapba, amelynek vezetőit országos, titkos választás útján választanak meg. Ez a szervezet kell irányítsa és felügyelje társadalombiztosítási, a nyugdíjbiztosítási rendszert is és ezen szerveződésbe kell betagolódjon valamennyi munkaügyi ellátó szerveződés, a területi és önkormányzati szociális és jóléti intézményei is. \r\nAz Országos Szakszervezeti Alap helyi tanácsokból és választott tisztségviselőkből álló testület, melynek helyi, térségi és regionális szerveződései is vannak.\r\n\r\n6. Újra rendszert kell váltani! \r\nDecentralizált politikai modellre van szükség. A harmadik köztársaság válsága egyenes következménye a centralizált politikai modellnek. Nem valódi demokrácia az, ahol négyévente eldönthetjük, hogy kik kapnak szinte korlátlan uralmat Magyarországon. Kiegyensúlyozott, nyugodt életünk csak akkor lehet, ha alapvetően átalakítjuk politikai rendszerünket, növeljük az önkormányzati mozgásteret, több embert vonunk be a helyi közéletbe, térségi szinteken is választott, valós mozgástérrel rendelkező testületek állnak fel és kormányzati szintű regionális szerveződések állnak fel. Ez lényegében egy szövetségi állam jellegű, azaz decentralizált, modell.\r\n \r\n7. Államadósság egy részének elengedésért harcolunk\r\nHazánk torz és méltatlan helyzetbe kerülésének egyik oka az indokolatlan és kamatos kamat következtében meglévő államadósság. A fejlődés egyik gátjának tekintjük, hogy hazánk bevételeinek jelentős része a nem valóságos gazdaságba vagy szociális ellátásra folyik, hanem nemlétező és értéket nem termelő pénzintézetek, bankok, világszervezetek finanszírozásáa folyik el. A Balpárt egyik első intézkedéseként újratárgyalná az államadósság mértékét és visszafizetési ütemét. Adósságunk csökkentése lételemünk egyik bázisa kell legyen.\r\n\r\n8. Igen Európára, nem a kétsebességű szuperállamra\r\nMagyarország európai ország, így természetes, hogy részt veszünk a kontinens integrációs folyamatában. Az azonban aggodalomra ad okot, hogy egy olyan szuperállam van kialakulóban, amely valós demokratikus kontroll nélkül hozhat döntéseket. \r\nOlyan világot kell építeni ahol mindenki boldogulhat saját szülőföldjén, de ha akar szabadon megválaszthatja lakhelyét. Együttműködve a kölcsönös előnyök alapján, nem pedig versengve egymás kárára próbálva előnyökhöz jutni. Ennek alapján nem akarjuk az európai közösséget felrobbantani, de minden erőnkkel azon fogunk dolgozni, hogy az átalakuljon a tőke érdekeit képviselő rendszerből az emberek nagy többségének érdekeit képviselő  Szociális Európa rendszerévé. Küzdünk az egyre nyilvánvalóbb módon kétsebességű modell ellen, ami miatt gazdag és szegény országokra szakad szét a kontinens. Támogatjuk a bérjövedelmek kiegyenlítődését és az Európai Minimálbér bevezetését. \r\n\r\n9. Katonailag független Magyarországot Európa közepén!\r\nEllenezzük a konfliktusok katonai megoldását, a háborúk soha nem oldanak meg semmit, csak hatalmas hasznot hajtanak a tőkének és rengeteg szenvedést okoznak az embereknek. Ennek megfelelően követeljük független államot, hazánk NATO-ból való kilépését követeljük.\r\n\r\n10. Támogatjuk a Feltétel Nélküli Alapjövedelem bevezetését\r\nA szolidáris társadalmi rendben senki sem maradhat élelem, meleg otthon, az internet nyilvánossága nélkül. Az ehhez szükséges javakat a társadalom tagjai maguknak megtermelik, úgy, hogy közben az adózási rendszeren keresztül gondoskodnak arról is, hogy a közösségi szolgáltatások is elérhetőek legyenek. A hagyományos munkásmozgalom legfőbb törekvése mindenkoron az volt, hogy munkanélküliség, betegség vagy öregkor esetén is a társadalom tagjai rendelkezzenek a tisztes létfenntartáshoz szükséges jövedelemmel. Munkanélküli ellátással, táppénzzel vagy nyugdíjjal. A Feltétel Nélküli Alapjövedelem egy koncepció, amelynek lényege, hogy kiváltja a sok néven nevezett ellátásokat egyetlen fix jövedelemmel, így nem lenne szükség családi pótlékra, társadalombiztosításra, nyugdíjra, a számtalan néven futó segélyekre. A koncepció jó és perspektivikus, de amíg pontosan nincs kidolgozva ez a dolog, amíg a bevezetése csak torzított formában merül fel, addig nem támogatjuk, hogy „FNA-jellegű” kezdeményezésekre hivatkozva bármilyen jóléti ellátást megvonjanak. Amíg az FNA nem realizálódik, addig támogatjuk egy sávosan biztosított munkanélküli segélyt, amely egy két hónapig a korábbi jövedelem száz százaléka, további négy hónapig annak hetven százaléka, azt követően pedig ötven százaléka kell legyen. Fél év munkanélküliség után a közösségi munkaügyi szolgáltatónak komplex képzést kell biztosítania, ezt követően pedig legalább egy évig tartó közérdekű foglalkoztatást képzettségi szintnek megfelelően.\r\n\r\n11. A közösségi közlekedés reformja, segíteni kell a munkába jutást\r\nA magyar gazdaság válságának egyik legfőbb oka a szabadrablás mellett az, hogy az illetékesek piaci alapokra próbálták helyezni a közlekedési vállalatokat, a költségvetés kieső adóbevételeit pedig a benzinárhoz csapott ilyen-olyan címkékkel ellátott adófajtákkal próbálták pótolni. Ezen hibás döntések miatt megfizethetetlenné vált a közlekedés, így a munkába járás is egy olyan országban, ahol korábban milliók ingáztak napi szinten lakhelyük és a munkahelyük között.\r\nMivel a környezetvédelem ügye mellett elkötelezettek vagyunk, így nem támogatjuk a radikális üzemanyagár csökkentést, azonban a közösségi közlekedés átfogó reformjával segíteni kívánjuk a munkavállalók mobilitásának növekedését. Kilencven százalékos közösségi támogatásra van szükség minden utazás alkalmával. Középtávon pedig biztosak vagyunk benne, hogy a teljesen ingyenes tömegközlekedésé a jövő.\r\n\r\nOTTHONT!\r\n\r\nA Balpárt szerint a fűtött, egészséges otthon alapjog. Az elmúlt negyedszázadban százezrek vesztették el lakásukat, házukat, milliós azok tábora, akik képtelenek megfizetni a lakhatás költségeit és egyre nyilvánvalóbban fenyegeti őket otthonuk elvesztése. Szintén százezres azok tábora, akik kifizethetetlen deviza kölcsönöket vettek fel.\r\nSajnos a fiatalok többségének esélye sincs arra, hogy olyan jövedelmet szerezzen, amelyből otthont tartson fenn, ezért többségük külföldre vándorol vagy kényszerből szüleivel él.\r\n\r\n12. Újra építeni kell szociális bérlakásokat!\r\nA reménytelen helyzetekből kiutat kell találni. Nem pusztán anyagi helyzettől függ, hogy kinek lehet és kinek nem lehet otthona. Az önkormányzatok többsége rossz gazdájának bizonyult a közösségi bérlakásoknak. Azok jelentős részét koncepció nélkül értékesítette, sokszor felmerült a korrupciógyanú. Az is gyakori probléma ezen a területen, hogy a rászorulókat szándékosan szorítják ki az önkormányzati bérlakásokból. \r\nAz Országos Szakszervezeti Alap által felügyelt és irányított Országos Szociális Bérlakás Hivatal felállítására van szükség, mely koordinálja a meglévő lakások igazságos felhasználását és új lakások felépítését kezdeményezi.\r\n\r\nITTHON!\r\n\r\n A magyar történelem oly gyakran ismétlődő válsághelyzeteiben a reménytelen helyzetbe taszított tömegek százezres nagyságrendben menekültek külföldre. Ma sincs ez másként. Mintegy félmillió honfitársunk keresi a boldogulás lehetőségét külföldön. Többségükben fiatalok, akik itthon képtelenek voltak fenntartani magukat és családjukat, képtelenek voltak otthont vásárolni vagy bérelni. Azért menekültek el, mert Magyarországon ellopták a jövőjüket. Őket egy torz és tolvaj politikai és gazdasági elit egyeduralma hajtotta a határainkon túlra. Ha ők tartósan kint maradnak, akkor az rendkívül komoly társadalmi katasztrófát fog okozni Magyarországon, mert fel fog borulni a társadalmi újraelosztási rendszer egésze. Mindent meg kell tenni azért, hogy a szociális migráció megálljon, a fiatal generációk itthon tudjanak boldogulni és gyarapodni. Ehhez szükséges, hogy a nyugati és keleti bérek értéke közelítsen egymáshoz. A Balpárt nemzetközi testvérpártjaival együtt többek között ezen a megoldáson is dolgozik európai síkon. Európa szociális kiegyenlítődése az egyetlen eszköz, amivel a szociális migráció megállítható.'), ('42', 'A kommentekhez tennék még (köszönet helyett) like, dislike, \"block\" (egyetértek, nem értek egyet, ellenzem) gombokat'), ('43', 'A felvetés teljesen jogos. Ami most a rendszerbe van integrálva az a JComment nevü Joomla kiegészités amit a NET -ről töltöttem le és integráltam a programba. Ha találok/találsz valahol egy jobb Joomla comment kiegészitő modult akkor le lehet cserélni, illetve ha nekem lesz rá időm(vagy valaki másnak) akkor ezt meg lehet próbálni tovább fejleszteni (nem lenne kis munka)'), ('45', 'Javaslatom az átláthatóság és könnyebb kezelhetőség jegyében írom.\r\nJó volna, ha a kezdőlapról nem csak témakörök szerint lehetne megközelíteni a dolgokat, hanem volnának ugyanazon a felületen ilyen \"gombok\", melyek kilistázz